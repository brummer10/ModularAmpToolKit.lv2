
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix opts:   <http://lv2plug.in/ns/ext/options#> .
@prefix time:   <http://lv2plug.in/ns/ext/time#>.
@prefix units:  <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:   <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:   <http://lv2plug.in/ns/ext/urid#> .
@prefix pprop:  <http://lv2plug.in/ns/ext/port-props#> .
@prefix midi:   <http://lv2plug.in/ns/ext/midi#> .
@prefix patch:  <http://lv2plug.in/ns/ext/patch#> .
@prefix work:  <http://lv2plug.in/ns/ext/worker#> .
@prefix bufsz: <http://lv2plug.in/ns/ext/buf-size#> .


<urn:brummer:PowerAmpImpulses>
   a lv2:Plugin ,
       lv2:SimulatorPlugin ;
   doap:maintainer [
       foaf:name "brummer";
       foaf:homepage <https://github.com/brummer10/ModularAmpToolKit.lv2>;
   ] ;
   doap:name "PowerAmpImpulses" ;
   lv2:project <urn:brummer:PowerAmpImpulses> ;
   lv2:requiredFeature urid:map ;
   lv2:optionalFeature lv2:hardRTCapable ;
   lv2:requiredFeature urid:map ,
       bufsz:boundedBlockLength ,
       work:schedule ;
   bufsz:minBlockLength 64 ;
   bufsz:maxBlockLength 8192 ;
   lv2:extensionData work:interface ;
   lv2:minorVersion 1 ;
   lv2:microVersion 1 ;


rdfs:comment """
PowerAmpImpulses, a set of bar bone Impulse Response Data for tone shaping
to match specific Power amplifier stacks.
In- and Out-put controls allow gain stacking.

Developed by Brummer
""";


   lv2:port  [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 0 ;
      lv2:symbol "in0" ;
      lv2:name "In0" ;
   ], [
      a lv2:AudioPort ,
           lv2:OutputPort ;
      lv2:index 1 ;
      lv2:symbol "out0" ;
      lv2:name "Out0" ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 2 ;
      lv2:designation lv2:enabled;
      lv2:portProperty lv2:toggled ;
      lv2:symbol "Bypass" ;
      lv2:name "bypass" ;
      lv2:default 1 ;
      lv2:minimum 0 ;
      lv2:maximum 1 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 3 ;
      lv2:symbol "Knob1" ;
      lv2:name "input" ;
      lv2:default 0.000000 ;
      lv2:minimum -20.000000 ;
      lv2:maximum 20.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 4 ;
      lv2:symbol "Knob2" ;
      lv2:name "tone" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 5 ;
      lv2:symbol "Combobox3" ;
      lv2:name "tube_style" ;
      lv2:default 0.0 ;
      lv2:minimum 0.0 ;
      lv2:maximum 19.0 ;
      lv2:portProperty lv2:integer ;
      lv2:portProperty lv2:enumeration ;
      lv2:scalePoint [rdfs:label "Blackat Leon"; rdf:value 0];
      lv2:scalePoint [rdfs:label "Cicognani Imperivm Luxury"; rdf:value 1];
      lv2:scalePoint [rdfs:label "DV Mark Triple 6"; rdf:value 2];
      lv2:scalePoint [rdfs:label "Engl Retro Tube"; rdf:value 3];
      lv2:scalePoint [rdfs:label "Engl Special Edition"; rdf:value 4];
      lv2:scalePoint [rdfs:label "EVH 5150 III"; rdf:value 5];
      lv2:scalePoint [rdfs:label "Fortin Natas"; rdf:value 6];
      lv2:scalePoint [rdfs:label "Kaos Sludge 15"; rdf:value 7];
      lv2:scalePoint [rdfs:label "Krank Krankenstein"; rdf:value 8];
      lv2:scalePoint [rdfs:label "Laney ironheart"; rdf:value 9];
      lv2:scalePoint [rdfs:label "MakosampCustomHatred"; rdf:value 10];
      lv2:scalePoint [rdfs:label "Marshall JMP 2203"; rdf:value 11];
      lv2:scalePoint [rdfs:label "Marshall MG 15"; rdf:value 12];
      lv2:scalePoint [rdfs:label "Mesa Boogie Mark V"; rdf:value 13];
      lv2:scalePoint [rdfs:label "Peavey Vypyr 15"; rdf:value 14];
      lv2:scalePoint [rdfs:label "Randall Satan"; rdf:value 15];
      lv2:scalePoint [rdfs:label "Randall thrasher"; rdf:value 16];
      lv2:scalePoint [rdfs:label "Splawn Nitro"; rdf:value 17];
      lv2:scalePoint [rdfs:label "Splawn Quick Rod"; rdf:value 18];
      lv2:scalePoint [rdfs:label "Taurus Stomphead"; rdf:value 19];
] .
