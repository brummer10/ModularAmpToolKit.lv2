
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix opts:   <http://lv2plug.in/ns/ext/options#> .
@prefix time:   <http://lv2plug.in/ns/ext/time#>.
@prefix units:  <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:   <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:   <http://lv2plug.in/ns/ext/urid#> .
@prefix pprop:  <http://lv2plug.in/ns/ext/port-props#> .
@prefix midi:   <http://lv2plug.in/ns/ext/midi#> .
@prefix patch:  <http://lv2plug.in/ns/ext/patch#> .
@prefix work:  <http://lv2plug.in/ns/ext/worker#> .
@prefix bufsz: <http://lv2plug.in/ns/ext/buf-size#> .


<urn:brummer:PreAmpImpulses>
   a lv2:Plugin ,
       lv2:SimulatorPlugin ;
   doap:maintainer [
       foaf:name "brummer";
       foaf:homepage <https://github.com/brummer10/ModularAmpToolKit.lv2>;
   ] ;
   doap:name "PreAmpImpulses" ;
   lv2:project <urn:brummer:PreAmpImpulses> ;
   lv2:requiredFeature urid:map ;
   lv2:optionalFeature lv2:hardRTCapable ;
   lv2:requiredFeature urid:map ,
       bufsz:boundedBlockLength ,
       work:schedule ;
   bufsz:minBlockLength 64 ;
   bufsz:maxBlockLength 8192 ;
   lv2:extensionData work:interface ;
   lv2:minorVersion 1 ;
   lv2:microVersion 1 ;


rdfs:comment """
PreAmpImpulses, a set of bar bone Impulse Response Data for tone shaping
to match specific Pre amplifier stacks.
In- and Out-put controls allow gain stacking.

Developed by Brummer
""";


   lv2:port  [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 0 ;
      lv2:symbol "in0" ;
      lv2:name "In0" ;
   ], [
      a lv2:AudioPort ,
           lv2:OutputPort ;
      lv2:index 1 ;
      lv2:symbol "out0" ;
      lv2:name "Out0" ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 2 ;
      lv2:designation lv2:enabled;
      lv2:portProperty lv2:toggled ;
      lv2:symbol "Bypass" ;
      lv2:name "bypass" ;
      lv2:default 1 ;
      lv2:minimum 0 ;
      lv2:maximum 1 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 3 ;
      lv2:symbol "Knob1" ;
      lv2:name "input" ;
      lv2:default 0.000000 ;
      lv2:minimum -20.000000 ;
      lv2:maximum 20.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 4 ;
      lv2:symbol "Knob2" ;
      lv2:name "output" ;
      lv2:default 0.000000 ;
      lv2:minimum -20.000000 ;
      lv2:maximum 20.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 5 ;
      lv2:symbol "Combobox3" ;
      lv2:name "tube_style" ;
      lv2:default 0.0 ;
      lv2:minimum 0.0 ;
      lv2:maximum 8.0 ;
      lv2:portProperty lv2:integer ;
      lv2:portProperty lv2:enumeration ;
      lv2:scalePoint [rdfs:label "AC30 Style"; rdf:value 0];
      lv2:scalePoint [rdfs:label "Bassman Style"; rdf:value 1];
      lv2:scalePoint [rdfs:label "Tube Style"; rdf:value 2];
      lv2:scalePoint [rdfs:label "JCM800 Style"; rdf:value 3];
      lv2:scalePoint [rdfs:label "JTM45 Style"; rdf:value 4];
      lv2:scalePoint [rdfs:label "Mesa Boogie Style"; rdf:value 5];
      lv2:scalePoint [rdfs:label "Boutique Style"; rdf:value 6];
      lv2:scalePoint [rdfs:label "Ampeg Style"; rdf:value 7];
      lv2:scalePoint [rdfs:label "Rectifier Style"; rdf:value 8];
] .
